    "@dnd-kit/modifiers": "8.x",
    "@dnd-kit/sortable": "9.x",
    "@popperjs/core": "2.x",
    "ahooks": "3.x",
    "polished": "4.x",
    "react": "^16.3.0 || 17.x || 18.x",
    "react-dock": "0.x",
    "react-dom": "^16.3.0 || 17.x || 18.x",
    "react-grid-system": "8.x",
    "react-popper": "2.x",
    "react-select": "5.x",
    "react-toastify": "8.x || 9.x || 10.x",
    "react-windowed-select": "5.1.0",
