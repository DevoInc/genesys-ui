    "ahooks": "3.x.x",
    "lodash": "4.x.x",
    "polished": "4.x.x",
    "react": "16.x || 17.x || 18.x",
    "react-dock": "0.x.x",
    "react-dom": "16.x || 17.x || 18.x",
    "react-grid-system": "8.x.x",
    "react-popper": "2.x.x",
    "react-select": "5.x.x",
    "react-split": "2.x.x",
