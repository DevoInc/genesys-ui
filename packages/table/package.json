{
  "name": "@devoinc/genesys-ui-table",
  "version": "1.8.0",
  "description": "Genesys composable table",
  "type": "module",
  "scripts": {
    "compile": "tsc --project tsconfig.json",
    "compile:watch": "npm run compile -- --watch",
    "build": "vite build && tsc --project tsconfig.build.json",
    "build:watch": "vite build --watch && tsc --project tsconfig.build.json",
    "depcruise": ". ../../depcruise.sh",
    "stryker": "npx stryker run"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@devoinc/genesys-ui": "^1.8.0",
    "@tanstack/react-virtual": "^3.0.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "react": "16.x || 17.x || 18.x"
  },
  "engine": {
    "node": ">=16",
    "npm": ">=7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DevoInc/genesys-ui.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
