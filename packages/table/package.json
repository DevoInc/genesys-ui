{
  "name": "@devoinc/genesys-ui-table",
  "version": "17.4.0",
  "description": "Genesys composable table",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "build": "tsup",
    "dpdm": "dpdm --exit-code circular:1 ./src/index.ts"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.js",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cjs",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@devoinc/genesys-icons": "4.x || 5.x || 6.x",
    "@devoinc/genesys-ui": "^17.4.0",
    "@devoinc/genesys-ui-datetime": "^17.4.0",
    "@tanstack/react-virtual": "3.x",
    "date-fns": ">=4.0",
    "@date-fns/tz": ">=1.0",
    "react": ">=17.0",
    "react-dom": ">=17.0",
    "styled-components": ">=5.0"
  },
  "engine": {
    "node": ">=16",
    "npm": ">=7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DevoInc/genesys-ui.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
