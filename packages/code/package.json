{
  "name": "@devoinc/genesys-ui-code",
  "version": "3.0.0",
  "description": "",
  "type": "module",
  "types": "./dist/types/src/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "tsc --project tsconfig.json",
    "compile:watch": "npm run compile -- --watch",
    "build": "vite build && tsc --project tsconfig.build.json",
    "build:watch": "vite build --watch && tsc --project tsconfig.build.json",
    "depcruise": "sh ../../depcruise.sh \"$npm_package_name\"",
    "analyzer": "npx vite-bundle-visualizer -o 'dist/stats.html'",
    "stryker": "npx stryker run"
  },
  "peerDependencies": {
    "@devoinc/genesys-brand-devo": "^6.1.1",
    "@devoinc/genesys-ui": "^3.0.0",
    "ahooks": "^3.7.8",
    "monaco-editor-core": "^0.24.0",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "styled-components": "^5.3.3"
  },
  "author": "",
  "license": "MIT",
  "engine": {
    "node": ">=16",
    "npm": ">=7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DevoInc/genesys-ui.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "file-saver": "^2.0.5"
  }
}
