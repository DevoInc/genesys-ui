    "stryker": "npx stryker run"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
