  "peerDependencies": {
    "@devoinc/genesys-ui": "^11.1.0",
    "polished": "4.x.x",
    "react": "16.x || 17.x || 18.x",
    "react-color": "2.x.x",
    "styled-components": "5.x.x || 6.x.x"
  },
  "engine": {
    "node": ">=16",
    "npm": ">=7"
  },
